
public class MainDisplay {
	
	public static Start ChooseStyle() {
		Start start = null;
		Book book=null;
		String type1="metal";
		String type2="wooden";
		
		if(type1.equalsIgnoreCase("METAL")) 
		{
			book=new MetalBook();
			book.createPen().write();
			
		}
		if(type1.equalsIgnoreCase("wooden")) 
		{
			book=new WoodenBook();
			book.createPen().write();
			
		}
		
		return start;
	}
	public static void main(String[] args) {
		
			ChooseStyle();
		
	}

}
